<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<transcoding-profiles>
    <profiles>
        <id>hq_2pass_video</id>
        <displayName>Video - High Quality 2-pass</displayName>
        <transcoderConfigurationItems xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:type="ffMpegTranscoderConfigurationItem">
            <niceness>19</niceness>
            <timeout>7200</timeout>
            <encoderArguments>-y -r 30000/1001 -b 2M -bt 4M -vcodec libx264 -pass 1 -vpre fastfirstpass -threads 0 -an</encoderArguments>
        </transcoderConfigurationItems>
        <transcoderConfigurationItems xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:type="ffMpegTranscoderConfigurationItem">
            <niceness>19</niceness>
            <timeout>7200</timeout>
            <encoderArguments>-y -r 30000/1001 -b 2M -bt 4M -vcodec libx264 -pass 2 -vpre hq -acodec libfaac -ac 2 -ar 48000 -ab 192k -threads 0</encoderArguments>
        </transcoderConfigurationItems>
        <mode>TWO_PASS</mode>
        <encodingFileExtension>.mp4</encodingFileExtension>
        <encodingMimeType>video/mpeg</encodingMimeType>
    </profiles>
