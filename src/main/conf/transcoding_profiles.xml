  <profiles>
    <id>segmented_video_high</id>
    <displayName>Video (High Quality) - HTTP Live Streaming</displayName>
    <transcoderConfigurationItems xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:type="ffMpegTranscoderConfigurationItem">
      <niceness>19</niceness>
      <timeout>7200</timeout>
      <encoderArguments>-v 1 -y -map 0:0 -map 0:1 -er 4 -f mpegts -acodec libfaac -ac 2 -ab 128k -ar 48000 -s 480x320 -vcodec libx264 -b 600k -flags +loop+mv4 -cmp 256 -partitions +parti4x4+partp8x8+partb8x8 -subq 7 -trellis 1 -refs 5 -coder 0 -me_range 16 -keyint_min 25 -sc_threshold 40 -i_qfactor 0.71 -bt 600k -maxrate 600k -bufsize 600k -rc_eq 'blurCplx^(1-qComp)' -qcomp 0.6 -qmin 10 -qmax 51 -qdiff 4 -level 30 -r 30 -g 90 -async 2</encoderArguments>
    </transcoderConfigurationItems>
    <transcoderConfigurationItems xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:type="segmenterTranscoderConfigurationItem">
      <niceness>19</niceness>
      <timeout>7200</timeout>
      <segmentDuration>10</segmentDuration>
      <segmentFilePrefix>segment</segmentFilePrefix>
      <playlistFileName>playlist.m3u8</playlistFileName>
      <httpPrefix>./</httpPrefix>
    </transcoderConfigurationItems>
    <mode>HTTP_SEGMENTED_VOD</mode>
    <encodingFileExtension/>
    <encodingMimeType>application/vnd.apple.mpegurl</encodingMimeType>
  </profiles>
